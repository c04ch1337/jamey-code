# Project Configuration
PROJECT_NAME=jamey

# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=jamey
POSTGRES_USER=jamey
POSTGRES_PASSWORD=change_me_in_production
POSTGRES_MAX_CONNECTIONS=10

# Vector Search Configuration
VECTOR_DIMENSION=1536
VECTOR_SIMILARITY_THRESHOLD=0.8
VECTOR_INDEX_TYPE=ivfflat  # Options: ivfflat, hnsw

# OpenRouter Configuration
OPENROUTER_API_KEY=your_api_key_here
OPENROUTER_DEFAULT_MODEL=claude-3-sonnet
OPENROUTER_ALLOWED_MODELS=claude-3-sonnet,gpt-4,gpt-3.5-turbo
OPENROUTER_API_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_TIMEOUT_SECONDS=30
OPENROUTER_MAX_RETRIES=3

# Runtime Configuration
LOG_LEVEL=info  # Options: debug, info, warn, error
ENABLE_REGISTRY_TOOL=true  # Windows-only feature
BACKUP_DIR=./backups
MAX_MEMORY_ENTRIES=1000
MEMORY_RETENTION_DAYS=30

# Security Configuration
API_KEY_REQUIRED=true
API_KEY=change_me_in_production
ALLOWED_ORIGINS=http://localhost:3000
ENABLE_CORS=true

# Tool Configuration
PROCESS_TOOL_ENABLED=true
PROCESS_TOOL_MAX_LIST=100
SELF_MODIFY_BACKUP_COUNT=5