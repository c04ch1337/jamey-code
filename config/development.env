# Development Configuration for Digital Twin Jamey

# Project Configuration
PROJECT_NAME=jamey_dev

# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=jamey_dev
POSTGRES_USER=jamey
POSTGRES_PASSWORD=dev_password
POSTGRES_MAX_CONNECTIONS=5

# Vector Search Configuration
VECTOR_DIMENSION=1536
VECTOR_SIMILARITY_THRESHOLD=0.8
VECTOR_INDEX_TYPE=ivfflat
MAX_MEMORY_ENTRIES=1000
MEMORY_RETENTION_DAYS=7

# OpenRouter Configuration
OPENROUTER_API_KEY=your_api_key_here
OPENROUTER_DEFAULT_MODEL=claude-3-sonnet
OPENROUTER_ALLOWED_MODELS=claude-3-sonnet,gpt-4,gpt-3.5-turbo
OPENROUTER_TIMEOUT_SECONDS=60
OPENROUTER_MAX_RETRIES=3

# Runtime Configuration
LOG_LEVEL=debug
ENABLE_REGISTRY_TOOL=true
BACKUP_DIR=./dev_backups
PROCESS_TOOL_ENABLED=true
PROCESS_TOOL_MAX_LIST=100
SELF_MODIFY_BACKUP_COUNT=10

# Security Configuration
API_KEY_REQUIRED=false
API_KEY=dev_key_123
ALLOWED_ORIGINS=http://localhost:3000
ENABLE_CORS=true